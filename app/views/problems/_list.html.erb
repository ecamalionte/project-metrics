<table class="table table-striped">
  <thead>
    <tr>
      <th><%= t("activerecord.attributes.problem.title") %></th>
      <th><%= t("activerecord.attributes.problem.desc") %></th>
    </tr>
  </thead>
  <tbody>
    <% @problems.each do |problem| %>
      <tr>
        <td><%= problem.title %></td>
        <td><%= problem.desc %></td>
      </tr>
    <% end %>
  </tbody>
</table>
